{
    "name": "circleBack",
    "description": "Chrome Extension for trackable LinkedIn networking",
    "version": "1.0.0",
    "manifest_version": 3,
    "action" : {
        "default_popup": "index.html",
        "default_title": "Toggle circleBack"
    },
    "icons": {
        "16" : "icons/icon16.png",
        "48" : "icons/icon192.png",
        "128" : "icons/icon512.png"
    },
    "permissions": {
        "activeTab"
    },
}
